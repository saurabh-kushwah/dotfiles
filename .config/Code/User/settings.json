{
  "debug.allowBreakpointsEverywhere": true,
  "debug.autoExpandLazyVariables": true,
  "debug.console.acceptSuggestionOnEnter": "on",
  "debug.console.closeOnEnd": true,
  "debug.console.wordWrap": false,
  "debug.disassemblyView.showSourceCode": false,
  "debug.internalConsoleOptions": "openOnSessionStart",
  "debug.openDebug": "openOnSessionStart",
  "debug.openExplorerOnEnd": true,
  "debug.showBreakpointsInOverviewRuler": true,
  "debug.showSubSessionsInToolBar": true,
  "debug.terminal.clearBeforeReusing": true,
  "debug.toolBarLocation": "docked",
  "diffEditor.codeLens": true,
  "diffEditor.experimental.useVersion2": true,
  "docker.containers.description": [],
  "docker.containers.label": "ContainerName",
  "docker.containers.sortBy": "Label",
  "docker.images.label": "RepositoryNameAndTag",
  "docker.images.sortBy": "Label",
  "editor.bracketPairColorization.enabled": true,
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.codeActionsOnSave": {
    "source.organizeImports": true
  },
  "editor.cursorSurroundingLines": 5,
  "editor.fontFamily": "'FiraCode NFM Retina', Consolas, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 13.5,
  "editor.formatOnSaveMode": "modifications",
  "editor.hideCursorInOverviewRuler": true,
  "editor.hover.delay": 500,
  "editor.maxTokenizationLineLength": 50000,
  "editor.minimap.autohide": true,
  "editor.parameterHints.cycle": true,
  "editor.quickSuggestions": {
    "comments": "on",
    "strings": "on"
  },
  "editor.renderLineHighlightOnlyWhenFocus": true,
  "editor.renderWhitespace": "trailing",
  "editor.smoothScrolling": true,
  "editor.stickyScroll.enabled": true,
  "editor.suggest.insertMode": "replace",
  "editor.suggest.localityBonus": true,
  "editor.suggest.preview": true,
  "editor.suggestSelection": "first",
  "editor.tabCompletion": "on",
  "editor.tabSize": 2,
  "editor.unfoldOnClickAfterEndOfLine": true,
  "editor.unusualLineTerminators": "auto",
  "editor.wordWrapColumn": 120,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "emmet.triggerExpansionOnTab": true,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "extensions.ignoreRecommendations": true,
  "files.associations": {
    "*.mod": "plaintext",
    "jenkinsfile": "groovy"
  },
  "files.defaultLanguage": "plaintext",
  "files.eol": "\n",
  "files.exclude": {
    "**/buildscripts/output": true,
    "**/node_modules/*/**": true,
    "**/vendor/*/**": true
  },
  "files.hotExit": "onExitAndWindowClose",
  "files.insertFinalNewline": true,
  "files.simpleDialog.enable": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/buildscipts/output**": true,
    "**/node_modules/*/**": true,
    "**/vendor/*/**": true
  },
  "git.countBadge": "off",
  "git.openRepositoryInParentFolders": "never",
  "go.buildFlags": [
    "-tags=linux"
  ],
  "go.coverOnSingleTest": true,
  "go.coverOnSingleTestFile": true,
  "go.coverShowCounts": true,
  "go.coverageOptions": "showCoveredCodeOnly",
  "go.delveConfig": {
    "hideSystemGoroutines": true,
    "substitutePath": []
  },
  "go.disableConcurrentTests": true,
  "go.editorContextMenuCommands": {
    "addImport": false,
    "addTags": false,
    "debugTestAtCursor": false,
    "generateTestForFunction": false,
    "playground": false,
    "testAtCursor": false,
    "testCoverage": false,
    "toggleTestFile": false
  },
  "go.inlayHints.compositeLiteralFields": true,
  "go.inlayHints.functionTypeParameters": true,
  "go.inlayHints.parameterNames": true,
  "go.lintFlags": [],
  "go.lintTool": "golangci-lint",
  "go.showWelcome": false,
  "go.survey.prompt": false,
  "go.testExplorer.concatenateMessages": false,
  "go.testFlags": [
    "-v",
    "-vet=all",
    "-count=1"
  ],
  "go.testTimeout": "6h",
  "go.toolsManagement.autoUpdate": true,
  "gopls": {
    "ui.semanticTokens": true
  },
  "java.codeGeneration.generateComments": true,
  "java.codeGeneration.hashCodeEquals.useInstanceof": true,
  "java.inlayHints.parameterNames.enabled": "all",
  "java.saveActions.organizeImports": true,
  "java.silentNotification": true,
  "launch": {
    "compounds": [],
    "configurations": [
      {
        "args": [
          "-test.v",
          "-test.timeout",
          "6h",
          "-test.run",
          "^${input:pickTest}$"
        ],
        "mode": "test",
        "name": "Go: Launch test function",
        "program": "${fileDirname}",
        "request": "launch",
        "type": "go"
      },
      {
        "mode": "debug",
        "name": "Go: Debug Current Package",
        "program": "${file}",
        "request": "launch",
        "type": "go"
      },
      {
        "mode": "local",
        "name": "Go: Attach to Process",
        "processId": "${command:pickProcess}",
        "request": "attach",
        "type": "go"
      },
      {
        "asRoot": true,
        "console": "integratedTerminal",
        "mode": "local",
        "name": "Go: Attach to Process (Root)",
        "processId": "${command:pickProcess}",
        "request": "attach",
        "suppressMultipleSessionWarning": true,
        "type": "go"
      },
      {
        "args": [],
        "name": "Python: Current File",
        "program": "${file}",
        "request": "launch",
        "type": "python"
      },
      {
        "args": [],
        "cwd": "${workspaceFolder}",
        "name": "LLDB: Attach to Process",
        "pid": "${command:pickProcess}",
        "request": "attach",
        "type": "lldb"
      }
    ],
    "inputs": [
      {
        "description": "Test Function Name",
        "id": "pickTest",
        "type": "promptString"
      }
    ]
  },
  "lldb.consoleMode": "evaluate",
  "lldb.showDisassembly": "never",
  "lldb.suppressUpdateNotifications": true,
  "markdown.preview.doubleClickToSwitchToEditor": false,
  "prettier.jsxSingleQuote": true,
  "prettier.printWidth": 120,
  "prettier.quoteProps": "consistent",
  "prettier.singleQuote": true,
  "python.experiments.enabled": false,
  "remote.SSH.defaultExtensions": [],
  "remote.SSH.localServerDownload": "off",
  "remote.SSH.lockfilesInTmp": true,
  "remote.SSH.remotePlatform": {
    "saurabh.kushwah": "linux"
  },
  "remote.autoForwardPorts": false,
  "rust-analyzer.assist.expressionFillDefault": "default",
  "rust-analyzer.check.targets": [
    "x86_64-unknown-linux-gnu"
  ],
  "rust-analyzer.debug.engine": "vadimcn.vscode-lldb",
  "rust-analyzer.files.watcher": "server",
  "rust-analyzer.hover.actions.implementations.enable": false,
  "rust-analyzer.inlayHints.bindingModeHints.enable": true,
  "rust-analyzer.lens.implementations.enable": false,
  "scm.countBadge": "off",
  "scm.defaultViewMode": "tree",
  "scm.diffDecorationsIgnoreTrimWhitespace": "true",
  "search.defaultViewMode": "tree",
  "search.quickOpen.includeSymbols": true,
  "search.useGlobalIgnoreFiles": true,
  "security.workspace.trust.banner": "never",
  "security.workspace.trust.enabled": false,
  "security.workspace.trust.startupPrompt": "never",
  "security.workspace.trust.untrustedFiles": "open",
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.defaultProfile.linux": "tmux",
  "terminal.integrated.drawBoldTextInBrightColors": false,
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "terminal.integrated.enablePersistentSessions": false,
  "terminal.integrated.env.linux": {},
  "terminal.integrated.fontWeightBold": "normal",
  "terminal.integrated.persistentSessionReviveProcess": "never",
  "terminal.integrated.profiles.linux": {
    "tmux": {
      "args": [
        "-c",
        "tmux new -ADs ${PWD##*/}"
      ],
      "icon": "terminal-tmux",
      "path": "bash"
    }
  },
  "terminal.integrated.shellIntegration.decorationsEnabled": "never",
  "terminal.integrated.shellIntegration.enabled": false,
  "terminal.integrated.showExitAlert": false,
  "terminal.integrated.smoothScrolling": true,
  "terminal.integrated.tabStopWidth": 4,
  "terminal.integrated.tabs.enabled": false,
  "terminal.integrated.tabs.location": "right",
  "testing.automaticallyOpenPeekView": "never",
  "testing.followRunningTest": false,
  "testing.openTesting": "neverOpen",
  "testing.showAllMessages": true,
  "window.commandCenter": false,
  "window.customMenuBarAltFocus": false,
  "window.enableMenuBarMnemonics": false,
  "window.experimental.windowControlsOverlay.enabled": false,
  "window.menuBarVisibility": "compact",
  "window.newWindowDimensions": "maximized",
  "window.title": "${folderPath}",
  "window.titleSeparator": "/",
  "workbench.colorCustomizations": {
    "[*]": {
      "editorInlayHint.background": "#0B0E1400",
      "editorInlayHint.foreground": "#ACB6BF8C",
      "editorInlayHint.parameterBackground": "#0B0E1400",
      "editorInlayHint.parameterForeground": "#ACB6BF8C",
      "editorInlayHint.typeBackground": "#0B0E1400",
      "editorInlayHint.typeForeground": "#ACB6BF8C"
    }
  },
  "workbench.colorTheme": "Ayu Dark",
  "workbench.commandPalette.experimental.suggestCommands": true,
  "workbench.commandPalette.preserveInput": true,
  "workbench.editor.decorations.badges": false,
  "workbench.editor.empty.hint": "hidden",
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.labelFormat": "short",
  "workbench.editor.untitled.hint": "hidden",
  "workbench.enableExperiments": false,
  "workbench.layoutControl.enabled": false,
  "workbench.list.smoothScrolling": true,
  "workbench.quickOpen.closeOnFocusLost": false,
  "workbench.quickOpen.preserveInput": true,
  "workbench.tips.enabled": false,
  "workbench.tree.indent": 4
}
